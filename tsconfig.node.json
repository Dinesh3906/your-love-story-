{
  "compilerOptions": {
    "target": "ES2023",
    "lib": [
      "ES2023"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "types": [
      "node"
    ],
    "skipLibCheck": true,
    "strict": true,
    "moduleDetection": "force",
    "resolveJsonModule": true,
    "noEmit": true,
    "allowJs": true
  },
  "include": [
    "vite.config.js"
  ]
}